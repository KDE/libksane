GET_FILENAME_COMPONENT(_IMPORT_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)
GET_FILENAME_COMPONENT(_IMPORT_PREFIX "${_IMPORT_PREFIX}/../.." ABSOLUTE)

find_library(KSANE_LIBRARY NAMES ksane PATHS ${_IMPORT_PREFIX} NO_DEFAULT_PATH)
SET(KSANE_LIBRARY "${KSANE_LIBRARY}" CACHE FILEPATH "KSane image scanning library")
SET(KSANE_INCLUDE_DIR "${_IMPORT_PREFIX}/include" CACHE PATH "Include path for KSane library")
SET(KSANE_FOUND TRUE)
